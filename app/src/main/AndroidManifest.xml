<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="pusat.android.makananbekuenak.com.aplikasi_marketer" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name=".EditdataPribadi" />
        <activity
            android:name=".HomeScreen"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="pusat.android.makananbekuenak.com.aplikasi_marketer.LoginActivity"
            android:theme="@style/AppTheme.Dark" />
        <activity
            android:name="pusat.android.makananbekuenak.com.aplikasi_marketer.SignupActivity"
            android:theme="@style/AppTheme.Dark" />
        <activity
            android:name=".RegistrasiMarketer"
            android:theme="@style/AppTheme.baru" >
        </activity>
        <activity android:name=".Pengaturan"
            android:label="Pengaturan"
            android:configChanges="orientation|screenSize"
            android:windowSoftInputMode="adjustPan|adjustResize" >
            <!--Coding Kembali Ke Menu Utama-->
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="pusat.android.makananbekuenak.com.aplikasi_marketer.HomeScreen" />
            <!-- END Coding Kembali Ke Menu Utama-->
        </activity>

        <activity android:name=".Laporan"
            android:label="Laporan"
            android:configChanges="orientation|screenSize"
            android:windowSoftInputMode="adjustPan|adjustResize" >
            <!--Coding Kembali Ke Menu Utama-->
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="pusat.android.makananbekuenak.com.aplikasi_marketer.HomeScreen" />
            <!-- END Coding Kembali Ke Menu Utama-->
        </activity>

        <activity android:name=".Entry_Order" />
        <activity android:name=".Entry_Order2" />
        <activity android:name=".Entry_Order3" />
        <activity android:name=".MainActivity" />
        <activity android:name=".Pesanan"
            android:label="Pesanan"
            android:configChanges="orientation|screenSize"
            android:windowSoftInputMode="adjustPan|adjustResize"
            android:exported="true">
            <!--Coding Kembali Ke Menu Utama-->
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="pusat.android.makananbekuenak.com.aplikasi_marketer.HomeScreen" />
            <!-- END Coding Kembali Ke Menu Utama-->
        </activity>
    </application>

</manifest>
